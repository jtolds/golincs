
CREATE TABLE dimensions (
	name text NOT NULL
,	idx integer NOT NULL
,	PRIMARY KEY (idx)
,	UNIQUE (name)
);

CREATE TABLE samples (
	batch text
,	cell_id text
,	pert_desc text
,	pert_dose text
,	pert_dose_unit text
,	pert_id text
,	pert_time text
,	pert_time_unit text
,	pert_type text
,	replicate_count text
,	sig_id text NOT NULL
,	PRIMARY KEY (sig_id)
);

